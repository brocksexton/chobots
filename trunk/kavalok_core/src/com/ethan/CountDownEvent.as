package com.ethan{	import flash.events.Event;		public class CountDownEvent extends Event 	{		public static const UPDATE:String = "update";				public var millisecondsLeft:Number;				public function CountDownEvent( type:String , millisecondsLeft:Number )		{			super(type);						this.millisecondsLeft = millisecondsLeft;		}				override public function clone():Event		{			return new CountDownEvent(type, millisecondsLeft);		}	}}