package com.kavalok.level{	import com.kavalok.Global;	import com.kavalok.dialogs.Dialogs;	import com.kavalok.gameplay.commands.RetriveStuffCommand;	import com.kavalok.level.Levels;	import com.kavalok.services.AdminService;	import com.kavalok.utils.Maths;		public class LevelUp	{				private var theLevels:Levels = new Levels();				public function LevelUp()		{						if (Global.charManager.experience >= theLevels.formula())			{				new AdminService().setLevel(Global.charManager.userId, Global.charManager.charLevel + 1);				Global.charManager.charLevel += 1;								LevelItem.instance.checkLevelStatus();								if (Global.charManager.charLevel == 20)				{										Dialogs.showOkDialog("Congrats on reaching level 20! If you're a junior, upgrade to citizen to keep earning Cho energy.", true);				}				else if (Global.charManager.charLevel == 60)				{					Dialogs.showOkDialog("Well, looks like this is the end of the road for now. You've reached level 60. Good work, buddy!");					Global.sendAchievement("ac32;","Level 60");				}				else				{					Dialogs.showOkDialog("Woohoo, you've leveled up! You are now level " + Global.charManager.charLevel + ".", true);				}							}			else			{				return;			}				}	}}