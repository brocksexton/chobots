package com.kavalok.remoting.commands{	import com.kavalok.Global;	import com.kavalok.remoting.RemoteConnection;	import com.kavalok.remoting.commands.ServerCommandBase;		public class removeModCommand extends ServerCommandBase	{				public function removeModCommand()		{		}				override public function execute():void		{			var className:String = String(parameter);			var clasName:String = 'com.kavalok.char.modifiers::' + className;			Global.charManager.removeModifier(clasName);		}		}}